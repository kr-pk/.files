#!/bin/sh

#Keyboard Layout
setxkbmap us &

#Turns Numlock Key On
xdotool key Num_Lock &

#Startup Processes
xcompmgr -c -C -t-5 -l-5 -r4.2 -o.55 &
unclutter --timeout 5 &
dwmblocks &
$HOME/.local/bin/setwp &
$HOME/.local/bin/boot &
dunst &
sxhkd &

#Starts MPD
mpd &

#Restarts DWM
while
	type dwm >/dev/null ; do dwm && continue || break;
done
