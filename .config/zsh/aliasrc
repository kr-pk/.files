alias hst="history 1 -1 | cut -c 8- | sort | uniq | fzf | tr -d '\n' | xclip -sel c"
alias dots='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias nmp="ncmpcpp"
alias srd="shred -uzn3"
alias mat="cmatrix"
alias mtp="jmtpfs"
alias tux="tabbed -cr 2 st -w '' -e tmux"
alias god="sudo"
alias my_dwm="git clone https://github.com/kr-pk/dwm.git"
alias my_dmenu="git clone https://github.com/kr-pk/dmenu.git"
alias my_st="git clone https://github.com/kr-pk/st.git"
alias my_blocks="git clone https://github.com/kr-pk/blocks.git"
alias my_dots="git clone https://github.com/kr-pk/.files.git"
alias my_arch="git clone https://github.com/kr-pk/arch.git"
alias remote="rdesktop-vrdp -g 1440x900 -P -z -x l -r sound:off -u"
alias pip-upgrade="pip list --outdated --format=columns | cut -d' ' -f1 | sed -n '1,2!p' | xargs -n1 pip3 install -U"
alias nscripts="cat /usr/share/nmap/scripts/script.db | less"
